{
  "presets": ["module:metro-react-native-babel-preset"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": [
          "./src"
        ],
        "alias": {
          "src": "./src",
          "assets": "./src/assets",
          "appStyles": "./src/styles",
          "shared": "./src/shared"
        }
      }
    ]
  ]
}